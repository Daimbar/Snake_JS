input.onButtonPressed(Button.A+Button.B, function () {
    basic.showString("3")
    basic.clearScreen()
    basic.pause(1000)
    basic.showString("2")
    basic.clearScreen()
    basic.pause(1000)
    basic.showString("1")
    basic.clearScreen()
    basic.pause(1000)
    let fruits = (randint(0, 4), randint(0,4))
    led.plot; (fruits);
})
